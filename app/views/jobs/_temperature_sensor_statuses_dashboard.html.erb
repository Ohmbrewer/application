<div class="panel panel-success">
  <div class="panel-heading">
    Temperature Sensors
  </div>
  <div class="panel-body">
    <div class="table-responsive">
        <table id="temperature_sensor_statuses_dashboard" class="temperature_sensor_statuses table table-hover table-striped">
          <thead>
            <tr>
              <th>Rhizome</th>
              <th>Temperature Sensor ID</th>
              <th>State</th>
              <th>Designated Stop Time</th>
              <th>Last Reading</th>
              <th>Last Read Time</th>
            </tr>
          </thead>
          <tbody>
          <%= render @temperature_sensor_statuses %>
          </tbody>
        </table>
    </div>
  </div>
</div>
