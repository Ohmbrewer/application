<%= f.label :data_pin %>
<%= f.select :data_pin,
             Rhizome.digital_pins.map { |et| ["D#{et}", et] },
             {prompt: '--'},
             class: 'form-control' %>
<%= f.label :onewire_index %>
<%= f.select :onewire_index,
             0..10,
             {prompt: '--'},
             class: 'form-control' %>
