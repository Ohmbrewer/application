<% provide(:title, 'Add Equipment Profile') %>
<h1>Add Equipment Profile</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@equipment_profile) do |f| %>
        <%= render 'fields', f: f, object: @equipment_profile %>
        <%= button_tag 'Add', type: :submit, class: 'btn btn-sm btn-primary' %>
        <%= link_to 'Cancel', equipment_profiles_path, class: 'btn btn-sm btn-danger' %>
    <% end %>
  </div>
</div>
